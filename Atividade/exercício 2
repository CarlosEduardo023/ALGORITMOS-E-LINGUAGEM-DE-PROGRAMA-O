 int[] v = new int[5];
            double receita = 0;
            for (int i = 0; i < 5; i++)
            {
                Console.WriteLine("Digite o valor pago: ");
                v[i] = int.Parse(Console.ReadLine());
            }
            
            for (int i = 0; i < 5; i++)
            {
                receita = receita + v[i];

            }
            Console.WriteLine("A receita total Ã©: " + receita );
