 static void Main(string[] args)
        {
            int[] a = new int[10];
            Console.WriteLine("escreva 10 numeros: ");
            for (int i = 0; i < 10; i++)
            {
                a[i] = int.Parse(Console.ReadLine());
            }
            Console.WriteLine("ordem inversa:");
            for( int i = 9; i >= 0; i--)
            {
                Console.WriteLine(a[i]);
            }
           
